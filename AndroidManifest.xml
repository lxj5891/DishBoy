<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.starwall.boy"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="17"/>

    <application
            android:name=".AppContext"
            android:label="@string/app_name" android:icon="@drawable/ic_launcher"
            android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen">

        <activity android:name=".app.MyAppActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".app.DeskActivity"/>
        <activity android:name=".app.ActionActivity"/>
        <activity android:name=".app.OrderActivity" android:label="order"/>
    </application>
</manifest>
